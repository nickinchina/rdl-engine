  <DataSets>
    <DataSet Name="Northwind">
      <Query>
        <CommandText>SELECT     Employee.LastName, SalesOrderHeader.SalesOrderID, Employee.FirstName, st.Name
FROM         Employee INNER JOIN
                      SalesOrderHeader ON Employee.EmployeeID = SalesOrderHeader.SalesPersonID INNER JOIN
                      SalesTerritory AS st ON SalesOrderHeader.TerritoryID = st.TerritoryID</CommandText>
        <DataSourceName>AdventureWorks</DataSourceName>
      </Query>
      <Fields>
        <Field Name="LastName">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>LastName</DataField>
